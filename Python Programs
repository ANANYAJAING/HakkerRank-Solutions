# 17.Find the string
Solutions:

def count_substring(string, sub_string):
    ms=len(string)
    ss=len(sub_string)
    c=0
    
    for i in range(ms-ss+1):
        if(string[i:(i+ss)]==sub_string):
            c=c+1
    return c

if __name__ == '__main__':
    string = input().strip()
    sub_string = input().strip()
    
    count = count_substring(string, sub_string)
    print(count)
