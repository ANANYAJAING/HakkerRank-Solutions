# 15.swap case
Solutions:

def swap_case(s):
    lst=[]
    for i in s:
        if i.isalpha():
            if i.islower():
                lst.append(i.upper())
            else:
                lst.append(i.lower())
        else:
            lst.append(i)
    return ''.join(lst)
if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)

